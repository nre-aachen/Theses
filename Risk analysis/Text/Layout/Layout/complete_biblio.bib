@article{Refsgaard2012,
	Annote = {Good overview of geological uncertainties and the comparison between geological and parametric uncertainties;
"However, the geological model becomes crucial in situa- tions, where groundwater models are used for extrapolation be- yond the calibration base, e.g., for transport predictions or for predictions of the effects of future changes in abstraction or cli- mate, when only head and discharge data for the present pumping situation or climate are available for calibration. In such situations, the geological structure uncertainty may often be the dominant source of uncertainty."
"The geological model has always been recognized as a very important element in groundwater modeling"
"Our experience suggests that use of more formalized methodologies for constructing stochastic geological models from basic geological and geostatistical knowledge, e.g., TProGS [23] or similar, may be a promising way forward'
"It is clear from our studies that all three main sources of geolog- ically related uncertainty, i.e., geological structures, effective parameters and local scale heterogeneity, will significantly influ- ence prediction uncertainty in groundwater modeling. The relative weight of the three sources of uncertainty will vary from case to case depending on the hydrogeological system, the data availabil- ity and the types of model predictions. As argued by Neuman and Wierenga [38], all three sources of uncertainty should be consid- ered. This can be done by exploring the effects of parameter uncer- tainty and local scale heterogeneity for a number of alternative geological models and then assess the joint uncertainty by use of Bayesian Model Averaging. We have not done so in our previous studies, and we are not aware of case studies where this has been done explicitly, so this is an impending challenge."




},
	Author = {Refsgaard, Jens Christian and Christensen, Steen and Sonnenborg, Torben O. and Seifert, Dorte and H{\o}jberg, Anker Lajer and Troldborg, Lars},
	Journal = {Advances in Water Resources},
	Month = feb,
	Number = {0},
	Pages = {36--50},
	Title = {{Review of strategies for handling geological uncertainty in groundwater flow and transport modeling}},
	Volume = {36},
	Year = {2012}}

