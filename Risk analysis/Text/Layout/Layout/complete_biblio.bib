	Number = {1},
	Owner = {flow},
	Pages = {525-544},
	Timestamp = {2015.09.29},
	Title = {Sequence stratigraphy and the habitat of hydrocarbons, Gippsland Basin, Australia},
	Type = {Journal Article},
